<%- include('../templates/header', {title}) %>


<form class="d-flex flex-column gap-3">
    <input type="text" class="form-control" name="full_name" required value="<%= user.full_name %>">
    <input type="text" class="form-control" name="email" required value="<%= user.email %>">
    <input type="text" class="form-control" name="password" required value="<%= user.password %>">
    <button type="submit" class="btn btn-success">Add</button>
    <a href="/users"><i data-feather="arrow-left-circle"></i> Go Back</a>
</form>



<%- include('../templates/footer') %>